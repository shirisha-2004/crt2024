import java.util.*;
public class LinearSearch{
public static int linearSearch(int[] a,int key){
  int n=a.length;
  for(int i=0;i<n;i++)
  {
   if(a[i]==key)
   return i; 
  }
return -1;
}
public static void main(String[] args){
 Scanner sc=new Scanner(System.in);
 int n=sc.nextInt();
 int[] a=new int[n];
 for(int i=0;i<n;i++)
 {
  a[i]=sc.nextInt();
 }
int key=sc.nextInt();
System.out.print(linearSearch(a,key));
}

}
